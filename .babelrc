{
  "plugins": [
    [
      "import",
      {
        "libraryName": "ant-design-vue",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ],
  "extends": [
    "plugin:vue/vue3-recommended"
  ],
  "rules": {
    "vue/no-multiple-template-root": "off",
    "vue/no-parsing-error": [
      2,
      {
        "x-invalid-end-tag": false
      }
    ]
  }
}